{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": [
    "src/components/domains/upload/ZKUploadForm.tsx",
    "src/components/domains/download/ZKDownloadForm.tsx",
    "src/components/domains/files/manager/components/*.tsx",
    "scripts/check-users.ts",
    "scripts/test-encryption-config.ts",
    "src/app/**/page.tsx",
    "src/app/**/layout.tsx",
    "src/app/**/loading.tsx",
    "src/app/**/error.tsx",
    "src/app/**/not-found.tsx",
    "**/index.ts",
    "**/index.tsx",
    "src/components/domains/admin/performance/PerformanceTest.tsx"
  ],
  "ignoreDependencies": [],
  "ignoreExportsUsedInFile": true,
  "workspaces": {
    ".": {
      "entry": [
        "src/app/**/{page,layout,loading,error,not-found,global-error,route,default}.{js,jsx,ts,tsx}",
        "src/middleware.{js,ts}",
        "src/instrumentation.{js,ts}",
        "next.config.{js,mjs,ts}"
      ],
      "project": [
        "src/**/*.{js,jsx,ts,tsx}",
        "scripts/**/*.{js,ts}",
        "!**/*.test.{js,jsx,ts,tsx}",
        "!**/*.spec.{js,jsx,ts,tsx}"
      ]
    }
  }
}
